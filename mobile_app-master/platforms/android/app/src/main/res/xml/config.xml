    <access origin="*" src="http://localhost:8101" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="http://localhost:8100" sessionid="43c9b58f" />
    <allow-navigation href="http://localhost:8101" sessionid="e78e9c06" />
    <allow-navigation href="http://localhost:8102" sessionid="4694c7b0" />
    <allow-navigation href="http://localhost:8103" sessionid="cc98cd5a" />
